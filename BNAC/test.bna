# set our iterator
SET i TO 0

# How many numbers to generate
SET count TO 7

SET total TO 0

# Loop 'count' number of times
^begin:
TEST i > count
GOTO end IF success

# Get random number 1 - 10
RANDOM x MAX 9
ADD 1 TO x
PRINT x

# Add to total and increment
ADD x TO total
WAIT 1
ADD 1 TO i

# Go back to loop condition
GOTO begin IF 1
^end:

SET average TO total
DIVIDE average BY count

# print average after waiting
WAIT 2
PRINT average