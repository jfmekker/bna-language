PRINT "Statement Test: SIZE"
SET errors TO 0

SIZE s OF "Hello, World!"
PRINT s
TEST s ! 13
ADD _test TO errors
SIZE s OF ""
PRINT s
TEST s ! 0
ADD _test TO errors
SET v TO "jwkdawdjnaw"
SIZE s OF v
PRINT s
TEST s ! 11
ADD _test TO errors

SIZE s OF ( 1 , 2 , "three" , ( 4 , 5 ) )
PRINT s
TEST s ! 4
ADD _test TO errors
SIZE s OF ( )
PRINT s
TEST s ! 0
ADD _test TO errors
LIST v SIZE 15
SIZE s OF v
PRINT s
TEST s ! 15
ADD _test TO errors


TEST errors > 0
GOTO bad IF _test
EXIT

^bad:
ERROR "One or more errors in SIZE test."