PRINT "Statement Test: MULTIPLY"
SET errors TO 0

SET a TO 5
MULTIPLY a BY 6
TEST a ! 30
ADD _test TO errors

SET a TO 5.1
MULTIPLY a BY 2.2
TEST a ! 11.22
ADD _test TO errors

SET a TO -10
MULTIPLY a BY -1
TEST a ! 10
ADD _test TO errors

SET a TO 10
MULTIPLY a BY 0.1
TEST a ! 1
ADD _test TO errors

SET a TO 7
MULTIPLY a BY a
TEST a ! 49
ADD _test TO errors

SET a TO 12.3456
MULTIPLY a BY 0
TEST a ! 0
ADD _test TO errors


TEST errors > 0
GOTO bad IF _test
EXIT

^bad:
ERROR "One or more errors in MULTIPLY test."
