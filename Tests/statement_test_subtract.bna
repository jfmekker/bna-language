PRINT "Statement Test: SUBTRACT"
SET errors TO 0

# Set integers
SET i1 TO 12345
SET i2 TO 0
SET i3 TO -12345

# Set floats
SET f1 TO 12345.6789
SET f2 TO 0.10101
SET f3 TO 0.0
SET f4 TO -0.10101
SET f5 TO -12345.6789

# Integer subtraction
SET x TO 0
SUBTRACT i1 FROM x
SUBTRACT i2 FROM x
SUBTRACT i3 FROM x
SUBTRACT 12345 FROM x
SUBTRACT 0 FROM x
SUBTRACT -0 FROM x
SUBTRACT -12345 FROM x
TEST x > 0
ADD _test TO errors
TEST x < 0
ADD _test TO errors

# Float subtraction
SET y TO 0
SUBTRACT f1 FROM y
SUBTRACT f2 FROM y
SUBTRACT f3 FROM y
SUBTRACT f4 FROM y
SUBTRACT f5 FROM y
SUBTRACT 12345.6789 FROM y
SUBTRACT 0.10101 FROM y
SUBTRACT 0.0 FROM y
SUBTRACT -0.0 FROM y
SUBTRACT -0.10101 FROM y
SUBTRACT -12345.6789 FROM y
TEST y > 0
ADD _test TO errors
TEST y < 0
ADD _test TO errors


